          <div class="table-container">
            <h2 style="color: #002147;">ALL Meet details</h2><br>
            <table style="border: 1px solid black; border-collapse: collapse; width: 100%;">
                <thead>
                  <tr style="background-color: rgb(11, 141, 167);">
                    <th style="color: black; text-align: center;">Contact Person</th>
                    <th style="color: black; text-align: center;">Email</th>
                    <th style="color: black; text-align: center;">Meeting Date</th>
                    <th style="color: black; text-align: center;">Meeting Time</th>
                    <th style="color: black; text-align: center;">Property Name</th>
                    <th style="color: black; text-align: center;">Scheduled At</th>
                  </tr>
                </thead>
                <tbody>
                  <% if (meetData.length > 0) { %>
                    <% meetData.forEach(meet => { %>
                      <tr>
                        <td style="border: 1px solid black; text-align: center;"><%= meet.contact_person_name %></td>
                        <td style="border: 1px solid black; text-align: center;"><%= meet.email_for_meet %></td>
                        <td style="border: 1px solid black; text-align: center;"><%= meet.meet_date %></td>
                        <td style="border: 1px solid black; text-align: center;"><%= meet.meet_time %></td>
                        <td style="border: 1px solid black; text-align: center;"><%= meet.pName %></td>
                        <td style="border: 1px solid black; text-align: center;"><%= meet.created_at %></td>
                      </tr>
                    <% }); %>
                  <% } else { %>
                    <tr>
                      <td colspan="6" style="text-align: center; border: 1px solid black;">No meetings scheduled</td>
                    </tr>
                  <% } %>
                </tbody>
              </table>
              
