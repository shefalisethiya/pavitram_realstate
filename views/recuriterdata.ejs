              <!-- graph -->
              <div class="row column2 graph margin_bottom_30">
                <div class="col-md-l2 col-lg-12">
                  <div class="white_shd full">
                    <div class="full graph_head">
                      <div class="heading1 margin_0">
                        <h2> job Seeker DATA</h2><br><br> 
                      </div>
                    </div>
                   <h2>Welcome, <%= fullName %>!</h2><br>

                   <table border="1">
                    <thead>
                      <tr>
                        <th>Full Name</th>
                        <th>Email</th>
                        <th>mobileNo</th>
                        <th>currentCity</th>
                        <th>organizationName</th>
                        <th>logo</th>
                        
                        <th>createdAt</th>
                      </tr>
                    </thead>
                    <tbody>
                      <% jobSeekers.forEach(jobseeker => { %>
                        <tr>
                          <td><%= jobseeker.fullName %></td>
                          <td><%= jobseeker.email %></td>
                          <td><%= jobseeker.mobileNo %></td>
                          <td><%= jobseeker.currentCity %></td>
                          <td><%= jobseeker.organizationName %></td>
                          <td>
                            <% if (jobseeker.logo) { %>
                              <!-- Thumbnail image with an onclick event to preview it -->
                              <img src="http://localhost:8085/download/<%= encodeURIComponent(jobseeker.logo) %>" 
                                   alt="Logo Preview" 
                                   style="max-width: 150px; cursor: pointer;" 
                                   onclick="previewImage(this.src)">
                            <% } else { %>
                              No logo Available
                            <% } %>
                          </td>
                          <td>
                            <%= new Date(jobseeker.createdAt).toLocaleString('en-US', {
                              year: 'numeric',
                              month: 'short',
                              day: 'numeric',
                              hour: '2-digit',
                              minute: '2-digit'
                            }) %>
                          </td>
                          <!-- Preview overlay (initially hidden) -->
                          <div id="previewOverlay" style="display:none; position:fixed; top:0; left:0; width:100%; height:100%; background:rgba(0,0,0,0.8); justify-content:center; align-items:center; z-index:1000;" onclick="closePreview()">
                            <img id="previewImg" src="" alt="Preview" style="max-width:90%; max-height:90%; box-shadow:0 0 10px #fff;">
                          </div>
                          
                          <script>
                            // Function to display the preview overlay with the selected image
                            function previewImage(src) {
                              var overlay = document.getElementById("previewOverlay");
                              var previewImg = document.getElementById("previewImg");
                              previewImg.src = src;
                              overlay.style.display = "flex";
                            }
                          
                            // Function to close the preview overlay
                            function closePreview() {
                              document.getElementById("previewOverlay").style.display = "none";
                            }
                          </script>
                          
                        </tr>
                      <% }) %>
