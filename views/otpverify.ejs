<script>
    function verifyOTP(event) {
      event.preventDefault();
      const enteredOtp = document.getElementById("otp").value.trim();
      const storedOtpData = localStorage.getItem("otp");
      if (!storedOtpData) {
        alert("OTP has expired or was not generated. Please request a new OTP.");
        return;
      }
      const { value: storedOtp, expires } = JSON.parse(storedOtpData);
      if (Date.now() > expires) {
        alert("OTP has expired! Please request a new OTP.");
        localStorage.removeItem("otp"); 
        return;
      }
      if (enteredOtp === storedOtp.toString()) {
        alert("✅ OTP verified successfully! Redirecting...");
        window.location.href = "/meet-schedule"; // Redirect to Meet Schedule page
      } else {
        alert("❌ Invalid OTP! Please enter the correct OTP.");
      }
    }
  </script>
